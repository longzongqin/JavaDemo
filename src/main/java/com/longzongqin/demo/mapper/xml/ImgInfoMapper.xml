<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.longzongqin.demo.mapper.ImgInfoMapper">

    <select id="getImgInfoByUser" resultType="com.longzongqin.demo.entity.ImgInfo">
        SELECT * FROM img_info WHERE status = 0 AND user_info_id = #{user_info_id} AND id = #{id}+11
    </select>
</mapper>
